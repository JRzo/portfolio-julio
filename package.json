	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "latest",
		"@testing-library/dom": "^10.4.0",
		"autoprefixer": "^10.4.17",
		"cross-env": "^7.0.3",
		"postcss": "^8.4.24",
		"postcss-cli": "^11.0.1",
		"tailwindcss": "^3.4.1"
	},
	"postcssOptions": {
		"postcss-preset-env": {
			"autoprefixer": {
				"flexbox": "no-2009"
			},
			"stage": 3
		},
		"plugins": [
			"tailwindcss",
			"autoprefixer",
			"@tailwindcss/postcss"
		]
	}
}
